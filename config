db_path=../../server.db

server {
    port=5000
    chunk_size=64 #in kB
    max_speed=8 # float value in Mbit
    debug=0 # 0/1 -> false/true
    # root directory which will be served
    root_dir=D:/
    # public domain name of the file server. Include the final slash!!
    domain=http://trinity.ingames.cz/stahovac/
}